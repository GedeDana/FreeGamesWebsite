{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
      "linebreak-style": "off",
      "no-console":"off",
      "import/no-extraneous-dependencies": ["error", { "devDependencies": true }],
      "no-underscore-dangle":"off",
      "no-restricted-globals":"off",
      "consistent-return": 0,
      "no-prototype-builtins": 0,
      "import/prefer-default-export": 0,
      "no-return-assign": 0,
      "class-methods-use-this":0,
      "no-new": 0,
      "no-undef":"off"
    }
}
